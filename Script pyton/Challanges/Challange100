from random import randint
from time import sleep


def sort(list):
    print('Sorted 5 numbers: ',end='')
    for cont in range (0,5):
        n = randint(1, 10)
        list.append(n)
        print(f'{n} ', end='', flush = True)
        sleep(0.3)
        print('Ok!')

def sumeven(list):
    sum = 0
    for v in list:
        if v % 2 == 0:
            sum += v
    print(f'The sum of even is {sum}')

num = []
sort(num)
sumeven(num)
