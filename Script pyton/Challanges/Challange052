#prime number
num = int(input('Enter a number: '))
t = 0
for c in range(1,num +1):
    if num % c == 0:
        print('\033[33m',end = ' ')
        t += 1
    else:
        print('\033[31m',end = ' ')
    print('{}'.format(c),end = ' ')
if t == 2:
    print('\nIs a prime number')
else:
    print('\nNot prime number')
    