a = input('Write anything:')
print('The primitive type is:',type(a))
print('Is only space?', a.isspace())
print('Is a number?' ,a.isnumeric())
print('Is a alfabetic?',a.isalpha())
print('Is a alfanumeric?',a.isalnum())
print('is uppercase?', a.isupper())
print('Is Lowercase?', a.islower())
print('Is title?', a.istitle())